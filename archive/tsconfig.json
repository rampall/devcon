{
  "extends": "../base.tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "module": "esnext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": [
        "./*"
      ],
      "lib/*": [
        "../../lib/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "src",
    "tina",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
