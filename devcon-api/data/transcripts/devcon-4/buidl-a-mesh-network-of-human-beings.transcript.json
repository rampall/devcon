			"timestamps": {
				"from": "00:01:52,240",
				"to": "00:01:54,400"
			},
			"offsets": {
				"from": 112240,
				"to": 114400
			},
			"text": " that are related to Ethereum,"
		},
		{
			"timestamps": {
				"from": "00:01:54,400",
				"to": "00:01:57,920"
			},
			"offsets": {
				"from": 114400,
				"to": 117920
			},
			"text": " but also have their own specifications and interests."
