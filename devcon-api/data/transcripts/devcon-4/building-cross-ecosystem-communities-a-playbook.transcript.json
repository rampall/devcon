			"timestamps": {
				"from": "00:07:26,480",
				"to": "00:07:28,600"
			},
			"offsets": {
				"from": 446480,
				"to": 448600
			},
			"text": " This is for marketing and building community."
		},
		{
			"timestamps": {
				"from": "00:07:28,600",
				"to": "00:07:32,480"
			},
			"offsets": {
				"from": 448600,
				"to": 452480
			},
			"text": " It's where you can go to find credible people to ask questions and it's all volunteer based."
		},
