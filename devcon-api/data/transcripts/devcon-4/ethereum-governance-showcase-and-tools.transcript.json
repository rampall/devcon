			"timestamps": {
				"from": "00:00:28,920",
				"to": "00:00:37,240"
			},
			"offsets": {
				"from": 28920,
				"to": 37240
			},
			"text": " I have one wife, Laura, top left, and three cats, top right is Vivian, bottom left is"
		},
		{
			"timestamps": {
				"from": "00:00:37,240",
				"to": "00:00:43,400"
			},
			"offsets": {
				"from": 37240,
				"to": 43400
			},
			"text": " Fry, and bottom right is Lulu."
