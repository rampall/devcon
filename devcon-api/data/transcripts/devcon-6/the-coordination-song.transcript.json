				"to": "00:00:22,720"
			},
			"offsets": {
				"from": 19560,
				"to": 22720
			},
			"text": " I felt it in my bones that we have this programmable substrate"
		},
		{
			"timestamps": {
				"from": "00:00:22,720",
				"to": "00:00:24,400"
			},
			"offsets": {
				"from": 22720,
				"to": 24400
			},
