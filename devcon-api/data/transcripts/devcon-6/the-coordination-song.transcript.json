				"to": "00:02:09,340"
			},
			"offsets": {
				"from": 128540,
				"to": 129340
			},
			"text": " Maybe."
		},
		{
			"timestamps": {
				"from": "00:02:09,340",
				"to": "00:02:11,480"
			},
			"offsets": {
