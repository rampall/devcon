		{
			"timestamps": {
				"from": "00:09:12,140",
				"to": "00:09:14,860"
			},
			"offsets": {
				"from": 552140,
				"to": 554860
			},
			"text": " So maybe it could be interesting to create"
		},
		{
			"timestamps": {
				"from": "00:09:14,860",
				"to": "00:09:19,020"
			},
			"offsets": {
				"from": 554860,
				"to": 559020
			},
			"text": " this type of options."
