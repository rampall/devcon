				"to": "00:00:27,240"
			},
			"offsets": {
				"from": 24440,
				"to": 27240
			},
			"text": " that we can hopefully all agree on."
		},
		{
			"timestamps": {
				"from": "00:00:28,440",
				"to": "00:00:32,600"
			},
			"offsets": {
				"from": 28440,
				"to": 32600
			},
