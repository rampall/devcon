			"timestamps": {
				"from": "00:01:00,320",
				"to": "00:01:05,040"
			},
			"offsets": {
				"from": 60320,
				"to": 65040
			},
			"text": " they're lacking awareness of this whole web-free space."
		},
		{
			"timestamps": {
