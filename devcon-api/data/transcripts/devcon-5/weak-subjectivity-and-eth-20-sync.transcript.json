				"from": "00:02:37,240",
				"to": "00:02:40,520"
			},
			"offsets": {
				"from": 157240,
				"to": 160520
			},
			"text": " or second you will offline for a long time."
		},
		{
			"timestamps": {
				"from": "00:02:40,520",
				"to": "00:02:41,520"
			},
			"offsets": {
				"from": 160520,
				"to": 161520
			},
			"text": " For how long?"
		},
		{
			"timestamps": {
				"from": "00:02:41,520",
				"to": "00:02:47,120"
			},
			"offsets": {
				"from": 161520,
				"to": 167120
			},
