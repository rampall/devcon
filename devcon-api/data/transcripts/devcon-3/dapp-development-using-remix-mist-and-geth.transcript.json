		{
			"timestamps": {
				"from": "00:09:32,000",
				"to": "00:09:38,000"
			},
			"offsets": {
				"from": 572000,
				"to": 578000
			},
			"text": " One really nice stuff that we have since the few months, if this is the swarm integration."
		},
		{
			"timestamps": {
				"from": "00:09:38,000",
				"to": "00:09:40,000"
			},
			"offsets": {
				"from": 578000,
				"to": 580000
			},
			"text": " Recool."
		},
