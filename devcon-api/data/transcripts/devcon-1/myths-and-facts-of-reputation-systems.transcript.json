				"from": "00:08:47,480",
				"to": "00:08:48,480"
			},
			"offsets": {
				"from": 527480,
				"to": 528480
			},
			"text": " Right?"
		},
		{
			"timestamps": {
				"from": "00:08:48,480",
				"to": "00:08:51,320"
			},
			"offsets": {
				"from": 528480,
				"to": 531320
			},
			"text": " So my weights are very different than your weights."
		},
		{
			"timestamps": {
				"from": "00:08:51,320",
				"to": "00:08:53,480"
			},
			"offsets": {
				"from": 531320,
				"to": 533480
			},
