			"timestamps": {
				"from": "00:07:24,280",
				"to": "00:07:30,280"
			},
			"offsets": {
				"from": 444280,
				"to": 450280
			},
			"text": " because you want to try to make it hard for people to make bad reputation statement or"
		},
