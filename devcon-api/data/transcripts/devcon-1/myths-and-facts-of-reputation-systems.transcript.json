		{
			"timestamps": {
				"from": "00:06:57,000",
				"to": "00:06:58,000"
			},
			"offsets": {
				"from": 417000,
				"to": 418000
			},
			"text": " Ownership."
		},
		{
			"timestamps": {
				"from": "00:06:58,000",
				"to": "00:06:59,000"
			},
			"offsets": {
				"from": 418000,
				"to": 419000
			},
			"text": " Ownership is very important."
		},
		{
			"timestamps": {
				"from": "00:06:59,000",
				"to": "00:07:01,000"
			},
			"offsets": {
				"from": 419000,
				"to": 421000
			},
			"text": " Who owns your reputation?"
