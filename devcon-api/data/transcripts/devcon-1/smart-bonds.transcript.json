		{
			"timestamps": {
				"from": "00:01:37,400",
				"to": "00:01:42,400"
			},
			"offsets": {
				"from": 97400,
				"to": 102400
			},
			"text": " And we also talk to infrastructure providers,"
		},
		{
			"timestamps": {
				"from": "00:01:42,400",
				"to": "00:01:45,400"
			},
			"offsets": {
				"from": 102400,
				"to": 105400
			},
			"text": " central banks, governments, regulators."
