				"to": "00:08:50,840"
			},
			"offsets": {
				"from": 528880,
				"to": 530840
			},
			"text": " and solutions and software."
		},
		{
			"timestamps": {
				"from": "00:08:50,840",
				"to": "00:08:53,160"
			},
			"offsets": {
				"from": 530840,
				"to": 533160
			},
			"text": " I think the same is true for application"
		},
		{
			"timestamps": {
				"from": "00:08:53,160",
				"to": "00:08:56,120"
			},
			"offsets": {
				"from": 533160,
				"to": 536120
			},
			"text": " training on the Golum network."
		},
