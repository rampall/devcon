			"timestamps": {
				"from": "00:07:52,720",
				"to": "00:07:57,760"
			},
			"offsets": {
				"from": 472720,
				"to": 477760
			},
			"text": " We're only going to send emails about truffle"
		},
		{
			"timestamps": {
				"from": "00:07:57,760",
				"to": "00:07:59,160"
			},
			"offsets": {
				"from": 477760,
				"to": 479160
			},
			"text": " and about the beta program."
		},
		{
			"timestamps": {
